2-
echo "Lucas" > cliente01.txt (less cliente01.txt)

3-
echo "Joinville" >> cliente01.txt

4-
mkdir clientes

5-
mv cliente01.txt clientes

6-
cd clientes
cp cliente01.txt cliente01.txt.bkp

7-
cd clientes
rm cliente01.txt

8-
vi cliente.script
	echo Lucas
	vi cliente01.txt
	echo "Lucas" > cliente01.txt
	echo "Joinville" >> cliente01.txt
	mkdir clientes
	mv cliente01.txt clientes
	cd clientes
	cp cliente01.txt cliente01.txt.bkp
	rm cliente01.txt

9-
rm -r clientes
chmmod +x cliente.script
./cliente.script

10-
